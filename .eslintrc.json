{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"space-before-function-paren": [
			"error",
			{
				"named": "never"
			}
		],
		"camelcase": [
			"error",
			{
				"properties": "never",
				"ignoreDestructuring": true
			}
		],
		"comma-dangle": [
			"error",
			{
				"imports": "always-multiline",
				"arrays": "always-multiline",
				"objects": "always-multiline"
			}
		],
		"semi": [
			"error",
			"always"
		],
		"no-mixed-spaces-and-tabs": "warn"
	}
}